    "Stages": [
        {
            "INDX - Stage index": 10, 
            "Log Entries": [
                {
                    "QSDT - Stage Flags": {}, 
                    "CNAM - Log Entry": "The widow Kolfinna wants my help in extracting wergild, the traditional retribution of the Nords, for the murder of her husband. I am to obtain Sigvatr the Strong's family heirloom, the gem Pinetear, and return it to Kolfinna at her dwelling on the western bank of the Iggnir River. All Kolfinna wants is Pinetear; I don't necessary have to kill Sigvatr the Strong.", 
                    "Result Script": {
                        "SCHR - Basic Script Data": {
                            "Unused": "00 00 00 00", 
                            "RefCount": 0, 
                            "CompiledSize": 0, 
                            "VariableCount": 0, 
                            "Type": "Quest:1"
                        }
                    }
                }
            ]
        }, 
        {
            "INDX - Stage index": 20, 
            "Log Entries": [
                {
                    "QSDT - Stage Flags": {}, 
                    "CNAM - Log Entry": "I have refused Kolfinna's request that I extract wergild from Sigvatr the Strong. I know better than to get involved is such a sensitive family matter.", 
                    "Result Script": {
                        "SCHR - Basic Script Data": {
                            "Unused": "00 00 00 00", 
                            "RefCount": 0, 
                            "CompiledSize": 0, 
                            "VariableCount": 0, 
                            "Type": "Quest:1"
                        }
                    }
                }
            ]
        }, 
        {
            "INDX - Stage index": 30, 
            "Log Entries": [
                {
                    "QSDT - Stage Flags": {}, 
                    "CNAM - Log Entry": "I have agreed to assist the widow Kolfinna in extracting wergild from Sigvatr the Strong. The barbarian was last seen hunting near the Altar of Thrond, a stone formation to the northwest of Kolfinna's dwelling, just west of the Harstrad River. I am to obtain Sigvatr's gem Pinetear and return it to Kolfinna. It matters not whether the barbarian lives or dies, so long as Kolfinna gets the gem.", 
                    "Result Script": {
                        "SCHR - Basic Script Data": {
                            "Unused": "00 00 00 00", 
                            "RefCount": 0, 
                            "CompiledSize": 0, 
                            "VariableCount": 0, 
                            "Type": "Quest:1"
                        }
                    }
                }
            ]
        }, 
        {
            "INDX - Stage index": 40, 
            "Log Entries": [
                {
                    "QSDT - Stage Flags": {}, 
                    "CNAM - Log Entry": "I spoke with Sigvatr the Strong and tried to coax Pinetear from his possession, but my attempt was a failure. If I can gain Sigvatr's friendship and then appeal to his sense of guilt, I may be able to get Pinetear without a fight.", 
                    "Result Script": {
                        "SCHR - Basic Script Data": {
                            "Unused": "00 00 00 00", 
                            "RefCount": 0, 
                            "CompiledSize": 0, 
                            "VariableCount": 0, 
                            "Type": "Quest:1"
                        }
                    }
                }
            ]
        }, 
        {
            "INDX - Stage index": 50, 
            "Log Entries": [
                {
                    "QSDT - Stage Flags": {}, 
                    "CNAM - Log Entry": "Sigvatr the Strong has been overcome with guilt for the death of Gustav Two-Teeth, and agrees to make amends. He has given me his ancestral gem Pinetear, which I am to present to Kolfinna as payment of wergild.", 
                    "Result Script": {
                        "SCHR - Basic Script Data": {
                            "Unused": "00 00 00 00", 
                            "RefCount": 0, 
                            "CompiledSize": 0, 
