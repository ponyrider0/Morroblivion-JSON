            "Log Entries": [
                {
                    "QSDT - Stage Flags": {}, 
                    "CNAM - Log Entry": "I have received the blade from gro-Gluk. It is a beautiful weapon, but it is not a flaming blade, as Almalexia had suggested it would be.", 
                    "Result Script": {
                        "SCHR - Basic Script Data": {
                            "Unused": "00 00 00 00", 
                            "RefCount": 0, 
                            "CompiledSize": 0, 
                            "VariableCount": 0, 
