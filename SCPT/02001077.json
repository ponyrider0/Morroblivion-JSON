    "SCTX - Script Source": "scn mwCWUScript\n\n;Main quest script for Cast-When-Used system\n\nfloat fQuestDelayTime\nref Ench\nref Item\nref ItemWeap\nshort Itemuses\nshort Itemusesmax\nshort controldisabled\nref ItemSpell\nshort onplayer\n\nshort ItemChargeMax\nshort ItemChargeCost\nShort ItemChargeCurrent\n\nstring_var IconString\n\nint FEAcode\nfloat FEAUseExp\nfloat FEAMult\n\narray_var spellList\nref firstSpell\n\nshort bResetToFirstSpell\n\nBegin MenuMode\n	if controldisabled\n		if IsControlDisabled 7\n			EnableControl 7\n		endif\n	endif\nEnd\n\nBegin GameMode\n\n	if onplayer\n		Player.Dispel Ench\n		SetNameEx \"%n\" Item ItemSpell\n		SetEventHandler \"OnSpellCast\" mwCWUCastEventHandler \"ref\"::player \"object\"::ItemSpell\n;		set IconString to GetBipedIconPath 0 Item\n		if (IsBipedIconPathValid 0 Item)\n			set IconString to GetBipedIconPath 0 Item\n		elseif (IsIconPathValid Item)\n			set IconString to GetIconPath Item\n		else\n			set IconString to GetIconPath Ench\n		endif\n		SetMenuStringValue \"hudmain_background\\hudmain_Magic_Icon\\filename|Menus\\Icons\\%z\" IconString 1004\n		sv_Destruct IconString\n		set onplayer to 0\n		printc \"%n - CWU System configured\", Item\n	endif\n\n	if Item != 0\n		if Player.GetEquipped Item == 0\n			set bResetTOFirstSpell to mwFixes.StrictMWEnchantmentSelection \n			if (bResetToFirstSpell == 0)\n				SelectPlayerSpell DefaultPlayerSpell\n			else\n				let spellList := player.GetSpells\n				let firstSpell := *spellList\n				SelectPlayerSpell firstSpell\n				let spellList := ar_Null\n			endif\n			RemoveEventHandler \"OnSpellCast\" mwCWUCastEventHandler\n			set Item to 0\n		endif\n	endif\n\n	if GetPlayerSpell == ItemSpell\n\n		if ItemChargeCurrent < ItemChargeCost\n			DisableControl 7\n			set controldisabled to 1\n		elseif (ItemChargeCurrent >= ItemChargeCost) && controldisabled == 1\n			EnableControl 7\n			set controldisabled to 0\n		endif\n\n		if controldisabled\n			if OnControlDown 7 && Player.IsCasting == 0\n				Message \"The enchanted item does not have enough charge.\"\n				PlaySound 0enchantSfail\n			endif\n		endif\n\n	elseif GetPlayerSpell != ItemSpell\n		if Item != 0\n			printc \"CWUScript (line 93): disabling %n(%i)\", Item, Item\n			set Item to 0\n			RemoveEventHandler \"OnSpellCast\" mwCWUCastEventHandler\n		endif\n		if controldisabled == 1\n			EnableControl 7\n			set controldisabled to 0\n		endif\n	endif\nEnd", 
