    "SCTX - Script Source": "SCN mwShrineImperialCultScript\n\n; Applied to Imperial Cult Altars\n\nshort button\nshort QuestionState\nshort blessingcost\n\nBegin OnActivate\n	if(Player.GetFactionRank 0ImperialSCult < 0); player not in faction\n		set blessingcost to 25\n	elseif(Player.GetFactionRank 0ImperialSCult == 0)||(Player.GetFactionRank 0ImperialSCult == 1) ; layman or novice\n		set blessingcost to 10\n	elseif(Player.GetFactionRank 0ImperialSCult > 1)\n		set blessingcost to 0\n	endif		\n\n	if Player.GetItemCount Gold001 < blessingcost\n		MessageBox \"You do not have enough gold to make a donation.\" \"OK\"\n		Return\n	endif\n\n	if ( QuestionState == 0 )\n		MessageBox \"Would you like to make a donation of %.0f Septims and receive a blessing?\" blessingcost \"Yes\" \"No\"\n		set QuestionState to 10\n	endif\n\nEnd\n\n\nBegin GameMode\n\n;Ask second question \n	if ( QuestionState == 10 )\n\n		Set button to GetButtonPressed\n\n		if ( button == 0 )\n			MessageBox \"What blessing do you ask for?\" \"Cure Disease\" \"Cure Blight Disease\" \"Cure Poison\" \"Restore Attributes\"\n			set QuestionState to 20\n		elseif ( button == 1 )\n			set QuestionState to 0\n		endif\n\n		Return\n\n	endif\n\n;perform cure or give message\n	if ( QuestionState == 20 )\n\n		Set button to GetButtonPressed\n\n		if ( button == 0 )\n			PreloadMagicEffect CUDI			\n			Cast fbmwCureDiseaseOtherSpell Player\n			Player.RemoveItem Gold001 blessingcost\n			Set QuestionState to 0\n		endif\n		if ( button == 1 )\n			PreloadMagicEffect CUDI			\n			Cast fbmwbmCureBlightAltar Player\n			Player.RemoveItem Gold001 blessingcost			\n			Set QuestionState to 0\n		endif\n\n		if ( button == 2 )\n			PreloadMagicEffect CUPA\n			Cast fbmwCurePoisonOtherSpell Player\n			Player.RemoveItem Gold001 blessingcost			\n			Set QuestionState to 0\n		endif\n\n		if ( button == 3 )\n			PreloadMagicEffect REAT\n			Cast 0restoreSattributes Player					; Restore all Attributes 100\n			Player.RemoveItem Gold001 blessingcost		\n			Set QuestionState to 0\n		endif\n\n	endif\n\nEnd\n;<CSEBlock>\n;<CSECaretPos> 30 </CSECaretPos>\n;</CSEBlock>", 
