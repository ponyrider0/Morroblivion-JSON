    "SCTX - Script Source": "Scn fbmwCO12Qscript\n\nshort falcoguard\nshort initialized\nfloat timer\n\nbegin gamemode\n\n	if falcoguard == 0\n		if GetStage fbmwCO12 >= 10\n			if timer < 10\n				set timer to timer + GetSecondsPassed\n			else\n				fbmwBaslodREF.Enable\n				fbmwBaslodREF.AddItem 0BonemoldBow 1\n				fbmwBaslodREF.AddItem fbmwDireViperarrow 5\n				fbmwBaslodREF.EquipItem 0BonemoldBow\n				fbmwBaslodREF.EquipItem fbmwDireViperarrow\n				fbmwBaslodREF.StartCombat fbmwFalcoREF\n				set timer to 0\n				set falcoguard to 1\n			endif\n		endif\n	endif\n\n	if GetStage fbmwCO12 == 10\n		if fbmwFalcoREF.GetLOS fbmwBaslodREF == 1\n			fbmwBaslodREF.StopCombat \n			fbmwBaslodREF.evp\n		elseif timer > 3\n			if falcoguard == 1\n				SetStage fbmwCO12 20\n				set timer to 0\n				set falcoguard to 2\n			endif\n		else\n			set timer to timer + GetSecondsPassed\n		endif\n	endif\n	if falcoguard == 2\n		if timer < 2\n			set timer to timer + GetSecondsPassed\n		else\n			set falcoguard to 3\n		endif\n	elseif falcoguard == 3\n		fbmwGarnasguardREF.StartCombat fbmwBaslodREF\n		fbmwAferguardREF.StartCombat fbmwBaslodREF\n		fbmwGratianguardREF.StartCombat fbmwBaslodREF\n		set falcoguard to 4\n	elseif falcoguard == 4\n		fbmwCimberSValerius.Enable\n		fbmwCapitonSPopillius.Enable\n		fbmwCimberSValerius.StartCombat fbmwFalcoREF\n		fbmwCapitonSPopillius.StartCombat fbmwFalcoREF\n		set falcoguard to 5\n	endif\n	if GetStage fbmwCO12 >= 20\n		if falcoguard >=4\n			if fbmwBaslodREF.GetDead == 1\n				fbmwGarnasguardREF.StartCombat fbmwCimberSValerius\n				fbmwGarnasguardREF.StartCombat fbmwCapitonSPopillius\n				fbmwAferguardREF.StartCombat fbmwCimberSValerius\n				fbmwAferguardREF.StartCombat fbmwCapitonSPopillius\n				fbmwGratianguardREF.StartCombat fbmwCimberSValerius\n				fbmwGratianguardREF.StartCombat fbmwCapitonSPopillius\n			endif\n		endif\n	endif\n	if falcoguard == 5\n		if fbmwCimberSValerius.GetDead == 1\n			if fbmwCapitonSPopillius.GetDead == 1\n				fbmwGarnasguardREF.StopCombat \n