		endif\n		endif\n	endif\n	if(Transform == 1)\n		Print \"running hircine's ring\"\n		if(GetPathOnce == 0)\n			Let modelpath := Actor.GetModelPath\n			set GetPathOnce to 1\n			print\"Skeleton path aquired \" + modelpath\n		endif\n		Actor.SetModelPath \"Characters\\_male\\skeletonWerewolf.nif\"\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakBackward_werewolf.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakFastBackward_werewolf.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakFastForward_werewolf.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakFastLeft_werewolf.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakFastRight_werewolf.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakForward_werewolf.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakIdle_werewolf.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakLeft_werewolf.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakRight_werewolf.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakTurnLeft_werewolf.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakTurnRight_werewolf.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\SwimBackward_werewolf.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\SwimFastBackward_werewolf.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\SwimForward_werewolf.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\SwimFastForward_werewolf.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\Idle.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\OneHandEquip.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\OneHandUnequip.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakOneHandEquip.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakOneHandUnequip.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\WalkForward.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\WalkTurnLeft.kf\" 1\n		Actor.ToggleSpecialAnim \"CurseOfHircine\\WalkTurnRight.kf\" 1\n		Let equippeditem := Actor.GetEquippedItems\n		Let i := 0\n		if eval ((ar_Size equippeditem) > 0)\n			while i < ar_Size equippeditem\n				Let item := equippeditem[i]\n				Actor.UnequipItemNS item\n				Let i += 1\n			loop				\n		endif\n		if(Actor == Player)\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\WalkFastForward_pc_werewolf.kf\" 1\n			set oldencoumbrancesetting to GetGameSetting fActorStrengthEncumbranceMult \n			SetNumericGameSetting fActorStrengthEncumbranceMult 9999\n			Actor.AddItemNS fbmwWereBodyPC 1\n			Actor.EquipItemSilent fbmwWereBodyPC\n			Actor.AddItemNS fbmwWereHeadPC 1\n			Actor.EquipItemSilent fbmwWereHeadPC\n			Actor.AddItemNS fbmwbmwerewolfclawPC 1\n			Actor.EquipItemSilent fbmwbmwerewolfclawPC\n			Actor.AddItemNS fbmwbmWerewolfClawShieldPC 1\n			Actor.EquipItemSilent fbmwbmWerewolfClawShieldPC\n			Actor.AddSpellNs 0werewolfSregeneration\n;			Actor.AddSpellNS 0werewolfSvision\n			Actor.AddSpellNS 0werewolfSresists\n			Actor.SetFactionRank fbmwWereFaction 0\n			set iswerewolf to 1\n			if ( Actor.IsThirdPerson )\n				Actor.Update3D\n			else\n				ToggleFirstPerson 0\n				Actor.Update3D\n				ToggleFirstPerson 1\n			endif\n			set attackclaws  to (4)*(GetGlobalValue fbmwbmclawcost)\n			SetAttackDamage attackclaws fbmwbmwerewolfclawPC\n			DisableControl 5\n			DisableControl 7\n			DisableControl 15\n;			DisableControl 16\n			DisableControl 17\n			DisableControl 18\n			DisableControl 19\n			DisableControl 20\n			DisableControl 21\n			DisableControl 22\n			DisableControl 23\n			DisableControl 24\n			DisableControl 25\n		elseif(Actor != Player)\n			Print \"running non player part of the script\"\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\WalkFastForward_werewolf.kf\" 1\n			Actor.AddItem fbmwWereBodyNPC 1\n			Actor.EquipItem fbmwWereBodyNPC 1\n			Actor.AddItem fbmwWereHeadNPC 1\n			Actor.EquipItem fbmwWereHeadNPC 1\n			Actor.AddItem fbmwbmwerewolfclaw 1\n			Actor.EquipItem fbmwbmwerewolfclaw 1\n			Actor.AddItem fbmwbmWerewolfClawShield 1\n			Actor.EquipItem fbmwbmWerewolfClawShield 1\n			Actor.AddSpell 0werewolfSregeneration\n			Actor.AddSpell 0werewolfSresists\n			Actor.SetFactionRank fbmwbmWerewolfFaction 0\n			Actor.Update3D\n			Actor.setav aggression 100\n			set attackclawsNPC  to (4)*(GetGlobalValue fbmwbmClawcostnpc)\n			SetAttackDamage attackclawsNPC fbmwbmwerewolfclaw\n			StartCombat Player\n		Endif\n		set Transform to 3\n	endif\n	if(Transform == 3)\n		if(newtime >= (time + 6))\n			set Transform to 2\n		endif\n		set newtime to GameHour\n	endif\n	if(Transform == 2)\n		if(Actor == Player)\n			Actor.RemoveItemNS fbmwWereBodyPC 1\n			Actor.RemoveItemNS fbmwWereHeadPC 1\n			Actor.RemoveItemNS fbmwbmWerewolfClawShieldPC 1\n			Actor.RemoveItemNS fbmwbmwerewolfclawPC 1\n			Actor.RemoveSpellNS 0werewolfSregeneration\n;			Actor.RemoveSpellNS 0werewolfSvision\n			Actor.RemoveSpellNS 0werewolfSresists\n			Actor.SetFactionRank fbmwWereFaction -1\n			SetNumericGameSetting fActorStrengthEncumbranceMult oldencoumbrancesetting\n;			Let keyring := ar_Find fbmwBMringhircine equippeditem ; Avoid player reequip the ring of hircine\n;			if (keyring != 0)\n;				ar_Erase equippeditem keyring\n:			endif\n			Let i := 0\n			if eval ((ar_Size equippeditem) > 0)\n				while i < ar_Size equippeditem\n					Let item := equippeditem[i]\n					Actor.EquipItem2NS item\n					Let i += 1\n				loop				\n			endif\n			Let equippeditem  := ar_Null\n			Actor.SetModelPath $modelpath \n			Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakBackward_werewolf.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakFastBackward_werewolf.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakFastForward_werewolf.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakFastLeft_werewolf.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakFastRight_werewolf.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakForward_werewolf.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakIdle_werewolf.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakLeft_werewolf.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakRight_werewolf.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakTurnLeft_werewolf.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakTurnRight_werewolf.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\SwimBackward_werewolf.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\SwimFastBackward_werewolf.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\SwimForward_werewolf.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\SwimFastForward_werewolf.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\Idle.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\OneHandEquip.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\OneHandUnequip.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakOneHandEquip.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\SneakOneHandUnequip.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\WalkForward.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\WalkTurnLeft.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\WalkTurnRight.kf\" 0\n			Actor.ToggleSpecialAnim \"CurseOfHircine\\WalkFastForward_pc_werewolf.kf\" 0\n			EnableControl 5\n			EnableControl 7\n			EnableControl 15\n;			EnableControl 16\n			EnableControl 17\n			EnableControl 18\n			EnableControl 19\n			EnableControl 20\n			EnableControl 21\n			EnableControl 22\n			EnableControl 23\n			EnableControl 24\n			EnableControl 25\n			if ( Actor.IsThirdPerson )\n				Actor.Update3D\n			else\n				ToggleFirstPerson 0\n				Actor.Update3D\n				ToggleFirstPerson 1\n			endif\n			set iswerewolf to 0\n			set time to 0\n			set newtime to 0\n			set Transform to 0\n		endif\n	endif\n	if(Actor == Player)\n		if(iswerewolf == 1)\n			Let crosshairRef := GetCrosshairRef\n			if(crosshairRef.IsDoor == 1)&&(crosshairRef.GetLocked == 0);To avoid being blocked in an interior.\n				EnableControl 5\n			else\n				DisableControl 5\n			endif\n		endif\n		if (iswerewolf==1) ;experimental fix for combat\n			Let potentialattacker := GetFirstRef 35  \n			while (potentialattacker)\n				if(potentialattacker.GetIsCreature != 1)&&(potentialattacker.GetDead == 0)  ; to avoid permanent detection values to dead npcs\n					if(potentialattacker.GetDetectionLevel Player == 3)&&(potentialattacker.GetInFaction fbmwbmWerewolfFaction == 0) ; so they will attack only when they \"see\" you\n						if(potentialattacker.GetActorValue Aggression >= potentialattacker.GetDisposition Player); to avoid being attacked by non morroblivion npcs\n							potentialattacker.StartCombat Player\n						endif\n					endif\n				endif	\n				Let potentialattacker := GetNextRef\n			loop\n		endif  ; End experiment\n	endif\nEnd\nBegin MenuMode 1002\n	if(iswerewolf == 1)	\n		CloseAllMenus\n	endif\nEnd\nBegin MenuMode 1003\n	if(iswerewolf == 1)	\n		CloseAllMenus\n	endif\nEnd\nBegin MenuMode 1022\n	if(iswerewolf == 1)	\n		CloseAllMenus\n	endif\nEnd\nBegin MenuMode 1023\n	if(iswerewolf == 1)	\n		CloseAllMenus\n	endif\nEnd\n\n\n\n\n;<CSEBlock>\n;<CSECaretPos> 27 </CSECaretPos>\n;</CSEBlock>", 
