    "SCTX - Script Source": "Scn mwTRcaveinScript\n\nshort initialized\nshort state\nshort button\nshort lastsec\n\nfloat myX\nfloat myY\nfloat myZ\nfloat myRotZ\nfloat timer\n\nbegin gamemode\n\n	if ( initialized == 0 )\n		set myX to GetPos X\n		set myY to GetPos Y\n		set myZ to GetPos z\n		set myRotZ to GetAngle z\n		set initialized to 1\n	endif\n\n;done\n	if ( state == 4 )\n		return\n	endif\nend\n\n;waiting for activation\n\nbegin OnActivate\n	if ( state == 0 )\n		if ( Player.GetItemCount 0dwemerUsatchel00 > 0 )\n			set state to 1	\n			MessageBox \"Place satchel charge?\" \"Yes\" \"No\"\n		endif\n	endif\n	return\nend\n\n;waiting for response\nbegin gamemode\n	if ( state == 1 )\n		set button to GetButtonPressed\n		if ( button == 1 )\n			set state to 0\n		elseif ( button == 0 )\n			Player.RemoveItem 0dwemerUsatchel00 1\n			MessageBox \"    5    \"\n			set state to 2\n		endif\n	endif\n\n;count down\n	if ( state == 2 )\n		set timer to timer + GetSecondsPassed\n		if ( timer > 5 )\n			set state to 3\n		elseif ( timer > 4 )\n			if ( lastsec != 4 )\n				MessageBox \"    1    \"\n				set lastsec to 4\n			endif\n		elseif ( timer > 3 )\n			if ( lastsec != 3 )\n				MessageBox \"    2    \"\n				set lastsec to 3\n			endif\n		elseif ( timer > 2 )\n			if ( lastsec != 2 )\n				MessageBox \"    3    \"\n				set lastsec to 2\n			endif\n		elseif ( timer > 1 )\n			if ( lastsec != 1 )\n				MessageBox \"    4    \"\n				set lastsec to 1\n			endif\n		endif	\n	endif\n\n;explosion\n	if ( state == 3 )\n		if ( GetDistance Player < 300 )\n			Player.kill\n		endif\n;PlayGroup Forward, 1\n		if Player.GetInCell BamzTAmschendVSRadacAsSForge == 1\n			fbmwTRCaveinActivatorREF.Cast fbmwTRCaveinExplosion fbmwTRCaveinActivatorREF\n			PlaySound 0Thunder1\n			fbmwTRCaveinREF.Disable\n			fbmwTRCaveinActivatorREF.disable\n		elseif Player.GetInCell BamzTAmschendVSPassageSofStheSWalker == 1\n			fbmwTRCaveinActivatorREF2.Cast fbmwTRCaveinExplosion fbmwTRCaveinActivatorREF2\n			PlaySound 0Thunder1\n			fbmwTRCaveinREF2.Disable\n			fbmwTRCaveinActivatorREF2.Disable\n		endif\n		set state to 0\n	endif\n\nend\n;<CSEBlock>\n;<CSECaretPos> 15 </CSECaretPos>\n;</CSEBlock>", 
