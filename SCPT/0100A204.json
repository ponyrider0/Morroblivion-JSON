    "SCTX - Script Source": "scn FBMWLustidrikeScript\r\n\r\nshort sayonce\r\n\r\nbegin onactivate\r\nif sayonce==1\r\n	if player.getAV intelligence<30\r\n	messagebox \"Back again? I dunno, pal. You look like you've had plenty. Or maybe you ALWAYS look that way.\" \"sujamma\"\r\n	messagebox \"Free drinks, f'lah. It's on the house. Only you gotta drink it here. Don't want you drinking and flying out there.\" \"Drink a mug of sujamma.\" \"Decline.\"\r\n		if getbuttonpressed==0\r\n		messagebox \"Sorry, pal. You've had enough. Go get some exercise or something.\"\r\n		elseif getbuttonpressed==1\r\n		messagebox \"Fine. You're the guest. Whatever you want.\"\r\n		endif\r\n	else\r\n	messagebox \"Back already? Well, go ahead, drink up. It's free.\" \"sujamma\"\r\n	messagebox \"Free drinks, f'lah. It's on the house. Only you gotta drink it here. Don't want you drinking and flying out there.\" \"Drink a mug of sujamma.\" \"Decline.\"\r\n		if getbuttonpressed==0\r\n		messagebox \"Sure. Here ya go....\"\r\n		cast 0LustidrikeUCocktail player\r\n		elseif getbuttonpressed==1\r\n		messagebox \"Fine. You're the guest. Whatever you want.\"\r\n		endif\r\n	endif\r\nelse\r\nmessagebox \"Wanna drink, buddy? Got anything you want, so long as it's sujamma.\" \"sujamma\"\r\nmessagebox \"Free drinks, f'lah. It's on the house. Only you gotta drink it here. Don't want you drinking and flying out there.\" \"Drink a mug of sujamma.\" \"Decline.\"\r\n		if getbuttonpressed==0\r\n		messagebox \"Sure. Here ya go....\"\r\n		cast 0LustidrikeUCocktail player\r\n		elseif getbuttonpressed==1\r\n		messagebox \"Fine. You're the guest. Whatever you want.\"\r\n		endif\r\nset sayonce to 1\r\nendif\r\nend", 
