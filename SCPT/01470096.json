    "SCTX - Script Source": "scriptName mwDAMolagBalStatueScript\n\nshort speech\nfloat timer\n\nbegin onactivate\n	\n	if GetStage fbmwDAMolagbal < 10\n		set speech to 1\n	elseif GetStage fbmwDAMolagbal == 20\n		set speech to 8\n	endif\nend\n\nbegin gamemode\n\n	if GetStage fbmwDAMolagbal >= 30\n		return\n	endif\n\n	set timer to ( timer - GetSecondsPassed )\n\n	if timer <= 0\n		if GetStage fbmwDAMolagbal < 10\n			if speech == 1\n				DisablePlayerControls\n				set timer to mwShrineOfMolagBalREF.Say mwDAMolagBalSpeech 1 mwDaedricSpeaker 1\n				set speech to 2\n			elseif speech == 2\n				set timer to mwShrineOfMolagBalREF.Say mwDAMolagBalSpeech 1 mwDaedricSpeaker 1\n				set speech to 3\n			elseif speech == 3\n				set timer to mwShrineOfMolagBalREF.Say mwDAMolagBalSpeech 1 mwDaedricSpeaker 1\n				set speech to 4\n			elseif speech == 4\n				set timer to mwShrineOfMolagBalREF.Say mwDAMolagBalSpeech 1 mwDaedricSpeaker 1\n				set speech to 5\n			elseif speech == 5\n				set timer to mwShrineOfMolagBalREF.Say mwDAMolagBalSpeech 1 mwDaedricSpeaker 1\n				set speech to 6\n			elseif speech == 6\n				set timer to mwShrineOfMolagBalREF.Say mwDAMolagBalSpeech 1 mwDaedricSpeaker 1\n				set speech to 7\n			elseif speech == 7\n				EnablePlayerControls\n				SetStage fbmwDAMolagbal 10\n				player.AddItem 0keyumentauna 1\n			endif\n		elseif GetStage fbmwDAMolagbal == 20\n			if speech == 8\n				DisablePlayerControls\n				set timer to mwShrineOfMolagBalREF.Say mwDAMolagBalSpeech 1 mwDaedricSpeaker 1\n				set speech to 9\n			elseif speech == 9\n				set timer to mwShrineOfMolagBalREF.Say mwDAMolagBalSpeech 1 mwDaedricSpeaker 1\n				set speech to 10\n			elseif speech == 10\n				EnablePlayerControls\n				player.AddItem 0macesofsmolagsbaluunique 1\n				SetStage fbmwDAMolagbal 30\n			endif\n		endif\n	endif\nend\n;<CSEBlock>\n;<CSECaretPos> 230 </CSECaretPos>\n;</CSEBlock>", 
