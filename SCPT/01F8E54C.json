    "SCTX - Script Source": "Scn fbmwTR08BeranoScript\n\nShort DoOnce\nShort TimerOnce\nfloat fQuestDelayTime\nfloat timer\nshort CL1\nshort CL2\n\nbegin gamemode\n\nset fQuestDelayTime to 0.001\n\n	if fbmwTr08Hlaalu.Forvenfree == 1 && getdisabled == 0\n		disable\n	endif\n\nif ( getinsamecell player == 1 )\n	if ( DoOnce == 0 )\n		set CL1 to player.getcrimegold\n		set DoOnce to 1	\n	endif\nendif\n\nif ( getdead == 1 )\n	; when we are killed...\n	if ( GetStage fbmwTR08Hlaalu >= 70 )\n		; ... and if the player has a writ for us...\n		if (GetStage fbmwTR08Hlaalu == 76 || GetStage fbmwTR08Hlaalu == 77 )\n			return\n		endif\n		\n;now we take into acount the time it takes bounty to update for a murder...\n		If TimerOnce ==1\n			return\n		elseif (timer > 5)\n			set TimerOnce to 1\n		else\n			set timer to timer + GetSecondsPassed\n		endif\n;and after this time has passed....\n\n	if timeronce ==1\n		set CL2 to player.getcrimegold\n		set CL2 to (CL2 - 40)\n		if ( CL2 > CL1 )\n			; ... append a journal entry if the execution is seen ...\n			setstage fbmwTR08Hlaalu 76\n		else\n			; ... append a journal entry if the execution is NOT seen ...\n			setstage fbmwTR08Hlaalu 77\n		endif\n	endif\n	endif\nendif\n\nEnd\n;<CSEBlock>\n;<CSECaretPos> 185 </CSECaretPos>\n;</CSEBlock>", 
