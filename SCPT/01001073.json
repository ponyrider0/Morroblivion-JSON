    "SCTX - Script Source": "scn mwOBMESlowfallEffectScript\n\nref actor\nfloat VelZ\nfloat newVelZ\nlong mag\nshort index\nfloat reduction\n\nbegin scripteffectstart\n	\n	set actor to GetSelf\n	set index to GetScriptActiveEffectIndex\n	set mag to GetNthActiveEffectMagnitude index\n	set reduction to 1 + 3 * ( mag / 100 )\n	\nend\n\n\nbegin scripteffectupdate\n\n	if actor.IsSwimming\n		return\n	endif\n\n	set VelZ to actor.GetVerticalVelocity\n	set newVelZ to ( VelZ / reduction )\n	if VelZ < 0\n		actor.SetVerticalVelocity newVelZ\n		ResetFallDamageTimer\n	endif\nend\n;<CSEBlock>\n;<CSECaretPos> 10 </CSECaretPos>\n;</CSEBlock>", 
