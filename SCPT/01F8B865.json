0 4F 6E 4D 61 67 69 63 45 66 66 65 63 74 48 69 74 05 00 52 07 00 19 00 00 00 15 00 0A 00 72 04 00 73 03 00 02 00 20 30 19 00 00 00 11 00 00 00", 
    "SCTX - Script Source": "scn fbmwbmWerewolfStart\nshort infectday\nshort doonce\nshort fQuestDelayTime\nshort DoOnceHandle\nshort success\n;should work now\nbegin gamemode\n	if(GetGameLoaded)\n		set DoOnceHandle to 0\n	endif\n	if (Player.IsSpellTarget 0werewolfSblood == 1) || (Player.IsSpellTarget fbmwbmwerewolfbloodab == 1)\n		if (fbmwBMAAAImAWere.werestatus == -1)\n			; you are now immune, so remove both effects\n			Player.RemoveSpellNS fbmwbmwerewolfbloodab\n			Player.Dispel 0werewolfSblood\n			set fbmwbmplayerisinfectedwerewolf to 0\n			return\n		endif\n		if(doonce == 0)\n			set infectday to GameDaysPassed\n			set doonce to 1\n		endif\n		if(Player.IsSpellTarget fbmwbmwerewolfbloodab == 1)&&(DoOnceHandle == 0)\n			MessageBox \"DEBUG: SeniesLupinus effect callback installed from WerewolfStart\"\n			set success to	SetEventHandler \"OnMagicEffectHit\" fbmwbmremovesenieslupinusability \"ref\"::Player \"object\"::\"CUDI\"\n			if (success == 1)\n				set DoOnceHandle to 1\n			endif\n		endif\n		set fQuestDelayTime to 0.5\n		set fbmwbmplayerisinfectedwerewolf to 1\n		if GameDaysPassed - infectday > 2\n			;;TODO use sunset sunrise climate values\n			if (GameHour > 21) ||(GameHour  < 6)\n				set fbmwBMAAAImAWere.werestatus to 1\n				StartQuest fbmwBMAAAImAWere\n			endif\n		endif\n	endif\n	if (Player.IsSpellTarget 0werewolfSblood == 0)&&(Player.IsSpellTarget fbmwbmwerewolfbloodab == 0)\n		set fbmwbmplayerisinfectedwerewolf to 0\n		set infectday to 0\n		set doonce to 0\n		if(Player.IsSpellTarget fbmwbmwerewolfbloodab == 0)\n			RemoveEventHandler \"OnMagicEffectHit\" fbmwbmremovesenieslupinusability\n		endif\n		; cured before becoming werewolf:\n		;    stop the ImAWere script but keep the WereStart quest running in case player gets infected again\n		set fbmwBMAAAImAWere.werestatus to 0\n	endif\n\nend\n;<CSEBlock>\n;<CSECaretPos> 1325 </CSECaretPos>\n;</CSEBlock>", 
