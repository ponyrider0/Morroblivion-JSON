    "SCTX - Script Source": "scn mwDAMalacathStatueScript\r\n\r\nshort speech\r\nfloat timer\r\n\r\nbegin onactivate\r\n\r\n	if GetStage fbmwDAMalacath < 10\r\n		if GetPCIsRace Orc == 1\r\n			set speech to 1\r\n		else\r\n			set speech to -1\r\n		endif\r\n	elseif GetStage fbmwDAMalacath == 60\r\n		set speech to 4\r\n	endif\r\n\r\nend\r\n\r\nbegin gamemode\r\n\r\n	if GetStage fbmwDAMalacath >= 70\r\n		return\r\n	endif\r\n\r\n	set timer to ( timer - GetSecondsPassed )\r\n\r\n	if timer <= 0\r\n		if GetStage fbmwDAMalacath < 10\r\n			if speech == 1\r\n				DisablePlayerControls\r\n				set timer to mwDAMalacathStatueREF.Say mwDAMalacathSpeech 1 mwDaedricSpeaker 1\r\n				set speech to 2\r\n			elseif speech == 2\r\n				set timer to mwDAMalacathStatueREF.Say mwDAMalacathSpeech 1 mwDaedricSpeaker 1\r\n				set speech to 3\r\n			elseif speech == -1\r\n				DisablePlayerControls\r\n				set timer to mwDAMalacathStatueREF.Say mwDAMalacathSpeech 1 mwDaedricSpeaker 1\r\n				set speech to -2\r\n			elseif speech == -2\r\n				set timer to mwDAMalacathStatueREF.Say mwDAMalacathSpeech 1 mwDaedricSpeaker 1\r\n				set speech to 3\r\n			elseif speech == 3\r\n				EnablePlayerControls\r\n				SetStage fbmwdamalacath 10\r\n				AddTopic 1oreynsbearclaw\r\n				AddTopic 1kharagsgrotkhar\r\n			endif\r\n		elseif GetStage fbmwDAMalacath == 60\r\n			if speech == 4\r\n				DisablePlayerControls\r\n				set timer to mwDAMalacathStatueREF.Say mwDAMalacathSpeech 1 mwDaedricSpeaker 1\r\n				set speech to 5\r\n			elseif speech == 5\r\n				set timer to mwDAMalacathStatueREF.Say mwDAMalacathSpeech 1 mwDaedricSpeaker 1\r\n				set speech to 6\r\n			elseif speech == 6\r\n				EnablePlayerControls\r\n				player.AddItem 0helmubearclawuunique 1\r\n				SetStage fbmwdamalacath 70\r\n			endif\r\n		endif\r\n	endif\r\nend\r\n;<CSEBlock>\r\n;<CSECaretPos> 1572 </CSECaretPos>\r\n;</CSEBlock>", 
