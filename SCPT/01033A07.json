 00 01 00 10 10 09 00 02 00 07 00 6E EC FF FF FF 19 00 00 00 00 10 1B 00 01 00 13 00 59 6F 75 20 66 65 65 6C 20 73 74 72 61 6E 67 65 2E 2E 2E 00 00 00 00 15 00 10 00 73 11 00 0B 00 20 58 3A 10 05 00 01 00 72 03 00 16 00 0E 00 01 00 0A 00 20 73 11 00 20 31 30 20 3E 3D 39 10 0A 00 02 00 72 03 00 6E 1E 00 00 00 17 00 02 00 01 00 39 10 0A 00 02 00 72 03 00 6E 19 00 00 00 19 00 00 00 15 00 07 00 73 04 00 02 00 20 31 15 00 07 00 73 03 00 02 00 20 30 19 00 00 00 11 00 00 00", 
    "SCTX - Script Source": "scn fbmwBittercupScript\n\n; Applied to Bittercup, Ald Redaynia.\n\nshort button\nshort messageOn\nshort drink\nshort gone\n\nshort str\nshort int\nshort wil\nshort agi\nshort spd\nshort endr\nshort per\nshort luc\n\nshort maxatt\nshort maxattval\nshort minatt\nshort minattval\nshort TGtest\nfloat timer\n\nbegin gamemode\nif gone == 1\n	Disable\n	set gone to 0\n	return\nendif\nend\n\n\nbegin onactivate\n	Set messageOn to 2\n\nif ( messageOn == 0 )\n	return\nendif\n\nif ( messageOn == 2 )\n	Set messageOn to 1\n	MessageBox \"Do you wish to drink from the Bitter Cup or to pick it up? Drinking from the cup will improve your best attribute and lower your worst. If you drink, the Cup is lost forever.\" \"Drink from the Bitter Cup.\" \"Pick it up.\"	\nendif\n\nEnd\n\n\nBegin Gamemode\n\nif ( messageOn == 1 )\n	set button to GetButtonPressed\n	if ( button == 0 )\n		; drink it\n		Set drink to 1\n		Set messageOn to 0\n	endif\n	if ( button == 1 )\n		; pick it up\n		Activate\n		Set messageOn to 0\n		return\n	endif\nendif\n\n; drink from the cup\nif ( drink == 1 )\n	player.addspell 0vampireSbloodSberne\n	; attributes are 'get' once, determine maxatt and minatt\n\n	; strength\n	set str to ( Player.getbaseav strength )\n	set maxatt to 1\n	set maxattval to str\n	set minatt to 1\n	set minattval to str\n\n	; intelligence\n	set int to ( Player.getbaseav intelligence )\n	if ( int > maxattval )\n		set maxatt to 2\n		set maxattval to int\n	endif\n	if ( int < minattval )\n		set minatt to 2\n		set minattval to int\n	endif\n\n	; willpower\n	set wil to ( Player.getbaseav willpower )\n	if ( wil > maxattval )\n		set maxatt to 3\n		set maxattval to wil\n	endif\n	if ( wil < minattval )\n		set minatt to 3\n		set minattval to wil\n	endif\n\n	; agility\n	set agi to ( Player.getbaseav agility )\n	if ( agi > maxattval )\n		set maxatt to 4\n		set maxattval to agi\n	endif\n	if ( agi < minattval )\n		set minatt to 4\n		set minattval to agi\n	endif\n\n	; speed\n	set spd to ( Player.getbaseav speed )\n	if ( spd > maxattval )\n		set maxatt to 5\n		set maxattval to spd\n	endif\n	if ( spd < minattval )\n		set minatt to 5\n		set minattval to spd\n	endif\n\n	; endurance\n	set endr to ( Player.getbaseav endurance )\n	if ( endr > maxattval )\n		set maxatt to 6\n		set maxattval to endr\n	endif\n	if ( endr < minattval )\n		set minatt to 6\n		set minattval to endr\n	endif\n\n	; personality\n	set per to ( Player.getbaseav personality)\n	if ( per > maxattval )\n		set maxatt to 7\n		set maxattval to per\n	endif\n	if ( per < minattval )\n		set minatt to 7\n		set minattval to per\n	endif\n\n	; luck\n	set luc to ( Player.getbaseav luck )\n	if ( luc > maxattval )\n		set maxatt to 8\n		set maxattval to luc\n	endif\n	if ( luc < minattval )\n		set minatt to 8\n		set minattval to luc\n	endif\n\n	; modify the selected attributes\n	; one cannot modatt using a variable. it does wierd things\n	if ( maxatt == 1 )\n		Player.modav strength, 20\n	endif\n\n	if ( minatt == 1 )\n		Player.modav strength, -20\n	endif\n\n	if ( maxatt == 2 )\n		Player.modav intelligence, 20\n	endif\n\n	if ( minatt == 2 )\n		Player.modav intelligence, -20\n	endif\n\n	if ( maxatt == 3 )\n		Player.modav willpower, 20\n	endif\n\n	if ( minatt == 3 )\n		Player.modav willpower, -20\n	endif\n\n	if ( maxatt == 4 )\n		Player.modav agility, 20\n	endif\n\n	if ( minatt == 4 )\n		Player.modav agility, -20\n	endif\n\n	if ( maxatt == 5 )\n		Player.modav speed, 20\n	endif\n\n	if ( minatt == 5 )\n		Player.modav speed, -20\n	endif\n\n	if ( maxatt == 6 )\n		Player.modav endurance, 20\n	endif\n\n	if ( minatt == 6 )\n		Player.modav endurance, -20\n	endif\n\n	if ( maxatt == 7 )\n		Player.modav personality 20\n	endif\n\n	if ( minatt == 7 )\n		Player.modav personality, -20\n	endif\n\n	if ( maxatt == 8 )\n		Player.modav luck, 20\n	endif\n\n	if ( minatt == 8 )\n		Player.modav luck, -20\n	endif\n\n	MessageBox \"You feel strange...\"\n	set TGtest to (getstage fbmwTGBitterBribe )\n	if ( TGtest >= 10 )\n		setstage fbmwTGBitterBribe 30\n	else\n		setstage fbmwTGBitterBribe 25\n	endif\n\n	set gone to 1\n	set drink to 0\n\nendif\n\nEnd\n;<CSEBlock>\n;<CSECaretPos> 136 </CSECaretPos>\n;</CSEBlock>", 
