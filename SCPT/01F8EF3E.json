    "SCTX - Script Source": "ScriptName mwOBMESanctuaryEffectScript\n\nref caster\nshort index\nlong magnitude\nlong casterPersonality\nfloat casterDefendBonus\nshort bIsEffectOn\nfloat ARBonusOn\nfloat ARBonusOff\nlong randVal\nshort delayFrames\n\nBegin ScriptEffectStart\n\n	set caster to GetSelf\n	set index to GetScriptActiveEffectIndex\n	set magnitude to GetNthActiveEffectMagnitude index\n	set casterPersonality to GetActorValue Personality\n	set casterDefendBonus to GetAVMod DefendBonus max\n	\n	set delayFrames to 0\n;	set ARBonusOn to (casterPersonality/50)*100\n	set ARBonusOn to 100\n	set ARBonusOff to (-1 * ARBonusOn)\n;	ModAVMod DefendBonus max ARBonusOn\n	set bIsEffectOn to 0\n;	Message \"Sanctuary started. Magnitude = %.0f\", magnitude\n	\nEnd\n\nBegin ScriptEffectFinish\n	if (bIsEffectOn == 0)\n;		Message \"Effect Off->Off\"\n		return\n	else\n;		Message \"Effect On->Off\"\n		ModAVMod DefendBonus script ARBonusOff\n		set bIsEffectOn to 0\n		return\n	endif\n	\nEnd\n\nBegin ScriptEffectUpdate\n;	set delayFrames to (ScriptEffectElapsedSeconds % 1)\n	if (delayFrames < 5)\n		set delayFrames to delayFrames + 1\n		return\n	else\n		set delayFrames to 0\n	endif\n	\n	set randVal to GetRandomPercent\n;	Message \"Rand = %.0f, Magnitude = %.0f\", randVal, magnitude\n	if (randVal > magnitude)\n		; turn off\n		if (bIsEffectOn == 0)\n			return\n		else\n;			PrintC \"Effect On->Off, %.0f frames\", delayFrames\n			ModAVMod DefendBonus script ARBonusOff\n			set bIsEffectOn to 0\n		endif\n		return\n	else\n		; turn on\n		if (bIsEffectOn == 1)\n			return\n		else\n;			Print \"Effect Off->On\"\n			ModAVMod DefendBonus script ARBonusOn\n			set bIsEffectOn to 1\n		endif\n	endif\n		\nEnd\n\n;<CSEBlock>\n;<CSECaretPos> 1581 </CSECaretPos>\n;</CSEBlock>", 
