    "SCTX - Script Source": "Scn fbmwMazedBandEndScript\n\nshort button\nshort messageOn\nshort reset\nshort OnPcEquip\n\n\nBegin onequip\n\n	set reset to 1\n	MessageBox \"Close the inventory to continue\"\n\nend\n\n\nbegin gamemode\n\n\n	if ( reset == 1 )\n		Set OnPcEquip to 0\n		MessageBox \"Use the Mazedband to teleport where?\" \"Vivec\" \"Mournhold\" \"Sotha Sil\" \"Nowhere\"\n		if ( GetStage fbmwTRSothaSil == 100 )\n			set messageOn to 2\n			set reset to 0\n		else\n			set messageOn to 1\n			set reset to 0\n		endif\n	endif\n\n	if ( messageOn == 1 )\n\n		set button to GetButtonPressed\n\n\n;vivec\n		if ( button == 0 )\n			PlaySound 0conjurationShit\n			set reset to 0\n			set messageOn to 0	\n			Player.PositionCell 12, 219, -501, 0 VivecVSHighSFane\n					\n\n;mournhold\n		elseif ( button == 1 )\n			PlaySound 0conjurationShit\n			set reset to 0\n			set messageOn to 0	\n			Player.PositionCell 0, -478, -645, 0 MournholdSTempleXSHighSChapel\n			\n\n;sotha sil\n		elseif ( button == 2 )\n			PlaySound 0conjurationShit\n			set reset to 0\n			set messageOn to 0\n			Player.PositionCell 3976, 4179, 12310, 0 SothaSSilVSDomeSofSSothaSSil\n\n\n		elseif ( button == 3 )\n			set reset to 0\n			set messageOn to 0	\nreturn\n\n		endif\n\n	endif\n\n	if ( messageOn == 2 )\n\n		set button to GetButtonPressed\n\n\n;vivec\n		if ( button == 0 )\n			Message \"You feel a powerful force interrupt the Mazed Band's spell and pull you toward Mournhold.\"\n			PlaySound 0conjurationShit\n			set reset to 0\n			set messageOn to 0	\n			Player.PositionCell 0, -478, -645, 0 MournholdSTempleXSHighSChapel\n			\n\n;mournhold\n		elseif ( button == 1 )\n			Message \"You feel a powerful force interrupt the Mazed Band's spell and pull you toward Mournhold.\"\n			PlaySound 0conjurationShit\n			set reset to 0\n			set messageOn to 0\n			Player.PositionCell 0, -478, -645, 0 MournholdSTempleXSHighSChapel\n				\n\n;sotha sil\n		elseif ( button == 2 )\n			Message \"You feel a powerful force interrupt the Mazed Band's spell and pull you toward Mournhold.\"\n			PlaySound 0conjurationShit\n			set reset to 0\n			set messageOn to 0	\n			Player.PositionCell 0, -478, -645, 0 MournholdSTempleXSHighSChapel\n\n\n		elseif ( button == 3 )\n			set reset to 0\n			set messageOn to 0	\n			return\n\n		endif\n\n	endif\n\n\n\nEnd\n;<CSEBlock>\n;<CSECaretPos> 224 </CSECaretPos>\n;</CSEBlock>", 
