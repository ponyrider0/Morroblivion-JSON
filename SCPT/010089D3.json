    "SCTX - Script Source": "scn fbmwChargenQuestScript\n\nfloat fQuestDelayTime\nshort AddGuildTopics\nshort JiubSpeak\nshort Guard01Stage\nshort GuardDockStage\nshort CensusStage\nshort GotPapers\nfloat talktimer\n\nBEGIN GameMode\n\n	set talktimer to ( talktimer - GetSecondsPassed )\n\n	if JiubSpeak > 0\n\n		if talktimer <= 0\n			if JiubSpeak == 1\n				DisablePlayerControls\n				set talktimer to mwJiubREF.SayTo Player mwCGTalkJiub\n				set JiubSpeak to 2\n			elseif JiubSpeak == 2\n				set talktimer to mwJiubREF.SayTo Player mwCGTalkJiub\n				set JiubSpeak to 3\n			elseif JiubSpeak == 3\n				set talktimer to mwJiubREF.SayTo Player mwCGTalkJiub\n				set JiubSpeak to -1\n			endif\n		endif\n\n	endif\n\n\n	if talktimer <= 0\n		if Guard01Stage == 2\n			set talktimer to mwCGGuard1REF.SayTo Player mwCGGuard01Talk 1\n			set Guard01Stage to 3\n		elseif Guard01Stage == 3\n			EnablePlayerControls\n			set Guard01Stage to 4\n			mwCGGuard1REF.evp\n		endif\n	endif\n\n	if GuardDockStage > 0\n\n		if talktimer <= 0\n			if GuardDockStage == 1\n				set talktimer to mwCGGuardDockREF.SayTo Player mwCGTalkGuardDock\n				set GuardDockStage to 2\n			elseif GuardDockStage == 2\n				set talktimer to mwCGGuardDockREF.SayTo Player mwCGTalkGuardDock\n				set GuardDockStage to 3\n			elseif GuardDockStage == 3\n				EnablePlayerControls\n				set GuardDockStage to 4\n				mwCGGuardDockREF.evp\n			endif\n		endif\n\n	endif\n\n	if CensusStage > 0\n\n		if talktimer <= 0\n			if CensusStage == 1\n				set talktimer to mwSocuciusErgallaREF.SayTo Player mwCGTalkSocuciusErgalla\n				set CensusStage to 2\n			elseif CensusStage == 2\n				SetStage fbmwChargen 10\n				set CensusStage to 3\n			elseif CensusStage == 3\n				set talktimer to mwSocuciusErgallaREF.SayTo Player mwCGTalkSocuciusErgalla\n				set CensusStage to 4\n			elseif CensusStage == 4\n				SetStage fbmwChargen 20\n				set CensusStage to 5\n			elseif CensusStage == 5\n				set talktimer to mwSocuciusErgallaREF.SayTo Player mwCGTalkSocuciusErgalla\n				set CensusStage to 6\n			elseif CensusStage == 6\n				EnablePlayerControls\n				mwSocuciusErgallaREF.evp\n				mwCGPapersREF.enable\n				set CensusStage to -1\n			endif\n		endif\n\n	endif\n\nEND\n;<CSEBlock>\n;<CSECaretPos> 70 </CSECaretPos>\n;</CSEBlock>", 
