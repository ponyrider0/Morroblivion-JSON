    "SCTX - Script Source": "scriptName mwDAMehrunesDagonStatueScript\r\n\r\nshort speech\r\nfloat timer\r\nshort button\r\nshort messageOn\r\n\r\nbegin onactivate\r\n	if GetStage fbmwDAMehrunes == 20 && player.GetItemCount 0rustyudaggeruunique > 0\r\n		SetStage fbmwDAMehrunes 30\r\n		set speech to 9\r\n	elseif GetStage fbmwDAMehrunes < 10\r\n		set speech to 1\r\n	endif\r\nend\r\n\r\nbegin gamemode\r\n\r\n	if GetStage fbmwDAMehrunes == 40 || GetStage fbmwDAMehrunes == 25\r\n		return\r\n	endif\r\n\r\n	if messageOn == 1\r\n\r\n		set button to GetButtonPressed\r\n\r\n		if ( button >= 0 ) \r\n			set messageOn to 0\r\n		endif\r\n\r\n		if button == 0\r\n			set speech to 4	;agree\r\n		elseif button == 1\r\n			set speech to 6	;refuse\r\n		endif\r\n	endif\r\n\r\n	set timer to ( timer - GetSecondsPassed )\r\n\r\n	if timer <= 0\r\n		if GetStage fbmwDAMehrunes < 10\r\n			if speech == 1\r\n				DisablePlayerControls\r\n				set timer to mwDAMehrunesDagonStatueREF.Say mwDAMehrunesDagonSpeech 1 mwDaedricSpeaker 1\r\n				set speech to 2\r\n			elseif speech == 2\r\n				SetStage fbmwDAMehrunes 10\r\n			endif\r\n		elseif GetStage fbmwDAMehrunes == 10\r\n			if speech == 2\r\n				set speech to 3\r\n				set messageon to 1\r\n				MessageBox \"Would you like a chance to prove your worth to me?\" \"I wish to prove my worth.\" \"I have no desire to deal with you, Daedra Prince.\"\r\n			elseif speech == 4\r\n				set timer to mwDAMehrunesDagonStatueREF.Say mwDAMehrunesDagonSpeech 1 mwDaedricSpeaker 1	;agree\r\n				set speech to 5\r\n			elseif speech == 5\r\n				set timer to mwDAMehrunesDagonStatueREF.Say mwDAMehrunesDagonSpeech 1 mwDaedricSpeaker 1\r\n				set speech to 7\r\n			elseif speech == 6\r\n				set timer to mwDAMehrunesDagonStatueREF.Say mwDAMehrunesDagonSpeech 1 mwDaedricSpeaker 1	;refuse\r\n				set speech to 8\r\n			elseif speech == 7\r\n				EnablePlayerControls\r\n				SetStage fbmwdamehrunes 20	;agree\r\n			elseif speech == 8\r\n				EnablePlayerControls\r\n				SetStage fbmwdamehrunes 25	;refuse\r\n			endif\r\n		elseif GetStage fbmwDAMehrunes == 30\r\n			if speech == 9\r\n				DisablePlayerControls\r\n				set timer to mwDAMehrunesDagonStatueREF.Say mwDAMehrunesDagonSpeech 1 mwDaedricSpeaker 1\r\n				set speech to 10\r\n			elseif speech == 10\r\n				EnablePlayerControls\r\n				player.AddItem 0mehrunesaurazoruunique 1\r\n				player.RemoveItem 0rustyudaggeruunique 1\r\n				SetStage fbmwDAMehrunes 40\r\n			endif\r\n		endif\r\n	endif\r\nend\r\n;<CSEBlock>\r\n;<CSECaretPos> 242 </CSECaretPos>\r\n;</CSEBlock>", 
