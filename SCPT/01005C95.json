    "SCTX - Script Source": "scn mwOBMESwiftSwimEffectScript\n\nref actor\nlong mag\nlong magneg\nshort index\nshort swim\n\nbegin scripteffectstart\n\n	set actor to GetSelf\n	set index to GetScriptActiveEffectIndex\n	set mag to GetNthActiveEffectMagnitude index\n	set magneg to mag * -1\n\nend\n\nbegin scripteffectupdate\n\n	if actor.IsSwimming && swim == 0\n		actor.modav speed mag\n		set swim to 1\n	elseif actor.IsSwimming == 0 && swim == 1\n		actor.modav speed magneg\n		set swim to 0\n	endif\nend\n\nbegin scripteffectfinish\n\n	if swim == 1\n		actor.modav speed magneg\n		set swim to 0\n	endif\n\nend\n;<CSEBlock>\n;<CSECaretPos> 10 </CSECaretPos>\n;</CSEBlock>", 
