    "SCTX - Script Source": "scriptName mwDAAzuraStatueScript\n\nshort speech\nfloat timer\n\nbegin onactivate\n\n	if GetStage fbmwDAAzura < 10\n		set speech to 1\n	endif\n\n	if GetStage fbmwDAAzura == 20\n		if player.GetItemCount fbmwsheogorathssignetring == 1\n			set speech to 6\n		endif\n	endif\n\n	if GetStage fbmwDAAzura == 25\n		set speech to 8\n	endif\nend\n\nbegin gamemode\n\n	if GetStage fbmwDAAzura >= 30\n		return\n	endif\n\n	set timer to ( timer - GetSecondsPassed )\n\n	if timer <= 0\n		if GetStage fbmwDAAzura < 10\n			if speech == 1 \n				DisablePlayerControls\n				set timer to mwDAAzureStatueREF.Say mwDAAzuraSpeech 1 mwDaedricSpeaker 1\n				set speech to 2\n			elseif speech == 2\n				set timer to mwDAAzureStatueREF.Say mwDAAzuraSpeech 1 mwDaedricSpeaker 1\n				set speech to 3\n			elseif speech == 3\n				set timer to mwDAAzureStatueREF.Say mwDAAzuraSpeech 1 mwDaedricSpeaker 1\n				set speech to 4\n			elseif speech == 4\n				set timer to mwDAAzureStatueREF.Say mwDAAzuraSpeech 1 mwDaedricSpeaker 1\n				set speech to 5\n			elseif speech == 5\n				EnablePlayerControls\n				SetStage fbmwdaazura 10\n			endif\n		elseif GetStage fbmwDAAzura == 20\n			if speech == 6\n				DisablePlayerControls\n				set timer to mwDAAzureStatueREF.Say mwDAAzuraSpeech 1 mwDaedricSpeaker 1\n				set speech to 7\n			elseif speech == 7\n				EnablePlayerControls\n				player.AddItem azurasstar 1\n				;player.RemoveItem fbmwsheogorathssignetring 1\n				SetStage fbmwdaazura 30\n			endif\n		elseif GetStage fbmwDAAzura == 25\n			if speech == 8\n				DisablePlayerControls\n				set timer to mwDAAzureStatueREF.Say mwDAAzuraSpeech 1 mwDaedricSpeaker 1\n				set speech to 9\n			elseif speech == 9\n				EnablePlayerControls\n				SetStage fbmwdaazura 40\n			endif\n		endif\n	endif\nend\n;<CSEBlock>\n;<CSECaretPos> 1366 </CSECaretPos>\n;</CSEBlock>", 
