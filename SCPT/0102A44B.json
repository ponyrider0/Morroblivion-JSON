    "SCTX - Script Source": "scn fbmwTRrChampionScript\nshort cheapshot\nshort KarrodfightStart\nshort Karrodbeaten\nshort Karrodbribe\nshort move\nshort Karrodmove\nshort FightDay\nshort StartHour\nshort moveonce\nshort doonce\nshort KingHit\nshort Mercenary\nshort atposition\n\nbegin gamemode\n\n\n	if ( GetStage fbmwTRChampion == 20 )\n		if ( KarrodfightStart == 0 )\n			if Player.GetInCell MournholdVSRoyalSPalaceSThroneSRoom != 1 \n;when player leaves room, send Karrod to bar\n				fbmwKarrodREF.PositionCell -37.431, 180.252, 134.399, 180, MournholdVSTheSWingedSGuar\n				Set KarrodfightStart to 1\n			endif\n		endif\n	endif\n\n;wait for one day to pass\n	if ( KarrodfightStart == 1 )\n		if ( GameDaysPassed == FightDay ) ||  ( ( GameDaysPassed == FightDay + 1 ) && ( GameHour < StartHour ) )\n			return\n		endif\n	endif\n\n;if karrod at bar...\n	if ( KarrodfightStart == 1 )\n;...do not move him back if PC is at bar\n		if ( Player.GetInCell MournholdVSTheSWingedSGuar == 1 )\n			return\n		endif\n\n;...do not move him back if PC in throne room\n		if ( Player.GetInCell MournholdVSRoyalSPalaceSThroneSRoom == 1 )\n			return\n		endif\n\n;...otherwise, move him back and prep for fight\n		if ( doonce == 0 )\n			fbmwKarrodREF.PositionCell 111.062, -76.603, -68.484, 0, MournholdVSRoyalSPalaceSThroneSRoom	\n			set KarrodfightStart to 2\n			set doonce to 1\n		endif\n	endif\n\n;wait until helseth starts fight\n	if ( KarrodfightStart < 3 )\n		return\n	endif\n\n	if atposition == 0\n		fbmwFaranoREF.evp\n		fbmwIranoREF.evp\n		fbmwOthrelothREF.evp\n		fbmwTienusREF.evp\n		fbmwGratusREF.evp\n		set atposition to 1\n	endif\n\n\n	if fbmwKingHelsethREF.GetDistance fbmwHelMarker <= 35 && moveonce != -1\n		set moveonce to 2\n		fbmwIranoREF.Look Player\n		fbmwOthrelothREF.Look Player\n		fbmwTienusREF.Look Player\n		fbmwKingHelsethREF.Look Player\n		fbmwGratusREF.Look Player\n		fbmwFaranoREF.Look Player\n\n	endif\n\n	if ( moveonce == 2 )\n		fbmwKingHelsethREF.Say 1duelbegin\n		fbmwKarrodREF.StartCombat Player\n		set moveonce to -1\n	endif\nend\n;<CSEBlock>\n;<CSECaretPos> 649 </CSECaretPos>\n;</CSEBlock>", 
