    "SCTX - Script Source": "scn R0BILLMTWritSeloth\r\n; log a journal entry for each successful 'hit' the player performs for the Morag Tong\r\n\r\n; script location: should be placed on each and every target of the Morag Tong, with \r\n;  changes to reflect the person it is running on.\r\n\r\nShort DoOnce\r\nShort TimerOnce\r\nfloat fQuestDelayTime\r\nfloat timer\r\nshort CL1\r\nshort CL2\r\n\r\nbegin gamemode\r\n\r\nset fQuestDelayTime to 0.001\r\n\r\nif ( getinsamecell player == 1 )\r\n	if ( DoOnce == 0 )\r\n		set CL1 to player.getcrimegold\r\n		set DoOnce to 1	\r\n	endif\r\nendif\r\n\r\nif ( getdead == 1 )\r\n	; when we are killed...\r\n	if ( GetStage fbmwMTWritVendu > 0 )\r\n		;... and if the player has a writ for us...\r\n		if (GetStage fbmwMTWritVendu == 75) || (GetStage fbmwMTWritVendu == 85)\r\n			return\r\n		endif\r\n		\r\n;now we take into acount the time it takes bounty to update for a murder...\r\n		If TimerOnce ==1\r\n			return\r\n		elseif (timer > 5)\r\n			set TimerOnce to 1\r\n		else\r\n			set timer to timer + GetSecondsPassed\r\n		endif\r\n;and after this time has passed....\r\n\r\n	if timeronce ==1\r\n		set CL2 to player.getcrimegold\r\n		set CL2 to (CL2 - 40)\r\n		if ( CL2 > CL1 )\r\n			; ... append a journal entry if the execution is seen ...\r\n			setstage fbmwMTWritVendu 75\r\n		else\r\n			; ... append a journal entry if the execution is NOT seen ...\r\n			setstage fbmwMTWritVendu 85\r\n		endif\r\n	endif\r\n	endif\r\nendif\r\n\r\nEnd\r\n;<CSEBlock>\r\n;<CSECaretPos> 1283 </CSECaretPos>\r\n;</CSEBlock>", 
