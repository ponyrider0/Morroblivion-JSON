    "SCTX - Script Source": "scn mwMorroDefaultQuestScript\n\n;Various settings for Morroblivion\nshort QuestMarkersEnabled\nshort HardcoreHarvestMuck\nshort HardcoreHarvestOre\nshort TauntEnabled\nshort FastTravelEnabled\nshort ErrorDetection\n\nshort RestoreGMST\nlong OldControl\nlong OldControl2\n\nbegin GameMode\n\n;=============================================================================\n;The following block is part of the system to display dynamic text in dialogue\n;=============================================================================\n\n	if RestoreGMST\n\n		SetStringGameSettingEX \"sPCControlsTextPrefix|push\"\n		SetAltControl 18 OldControl\n		SetAltControl 19 OldControl2\n		SetStringGameSettingEX \"sMouseButton7|Button 7\"\n\n		set RestoreGMST to 0\n\n	endif\n\n;===================================================================\n;The following block automatically corrects PlayerInMorrowind global\n;===================================================================\n\n\n	if Player.GetInWorldspace WrldMorrowind == 1 || Player.GetInCell Mournhold == 1\n		if PlayerInMorrowind == 0\n			set PlayerInMorrowind to 1\n		endif\n	elseif Player.GetInWorldspace Tamriel == 1 || GetPlayerInSEWorld == 1\n		if PlayerInMorrowind == 1\n			set PlayerInMorrowind to 0\n		endif\n	endif\n\n;===============================================================================\n;The block below handles display of Morroblivion stats in player Misc Stat menu.\n;Will do nothing if AddActorValues OBSE plugin is not installed.\n;===============================================================\n\n	if IsPluginInstalled \"AddActorValues\" == 0\n		return\n	else\n		if eval (( Player.GetAVModF mwAVSlavesFreed base ) != FreedSlavesCounter )\n			Player.SetAVModF mwAVSlavesFreed base FreedSlavesCounter\n			if Player.GetAVModF mwAVSlavesFreed base == 0\n				SetQuestItem 0 mwAVSlavesFreed\n			else\n				SetQuestItem 1 mwAVSlavesFreed\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVThreadsWebspinner base ) != ThreadsWebspinner )\n			Player.SetAVModF mwAVThreadsWebspinner base ThreadsWebspinner\n			if Player.GetAVModF mwAVThreadsWebspinner base == 0\n				SetQuestItem 0 mwAVThreadsWebspinner\n			else\n				SetQuestItem 1 mwAVThreadsWebspinner\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepHT base ) != fbmwHTAdvancement.TelvanniReputation )\n			Player.SetAVModF mwAVRepHT base fbmwHTAdvancement.TelvanniReputation\n			if Player.GetAVModF mwAVRepHT base == 0\n				SetQuestItem 0 mwAVRepHT\n			else\n				SetQuestItem 1 mwAVRepHT\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepHH base ) != fbmwHHAdvancement.HlaaluReputation )\n			Player.SetAVModF mwAVRepHH base fbmwHHAdvancement.HlaaluReputation\n			if Player.GetAVModF mwAVRepHH base == 0\n				SetQuestItem 0 mwAVRepHH\n			else\n				SetQuestItem 1 mwAVRepHH\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepHR base ) != fbmwHRAdvancement.RedoranReputation )\n			Player.SetAVModF mwAVRepHR base fbmwHRAdvancement.RedoranReputation\n			if Player.GetAVModF mwAVRepHR base == 0\n				SetQuestItem 0 mwAVRepHR\n			else\n				SetQuestItem 1 mwAVRepHR\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepMG base ) != fbmwMGAdvancement.MageReputation )\n			Player.SetAVModF mwAVRepMG base fbmwMGAdvancement.MageReputation\n			if Player.GetAVModF mwAVRepMG base == 0\n				SetQuestItem 0 mwAVRepMG\n			else\n				SetQuestItem 1 mwAVRepMG\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepFG base ) != fbmwFGAdvancement.FighterReputation )\n			Player.SetAVModF mwAVRepFG base fbmwFGAdvancement.FighterReputation\n			if Player.GetAVModF mwAVRepFG base == 0\n				SetQuestItem 0 mwAVRepFG\n			else\n				SetQuestItem 1 mwAVRepFG\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepTG base ) != fbmwTGAdvancement.ThiefReputation )\n			Player.SetAVModF mwAVRepTG base fbmwTGAdvancement.ThiefReputation\n			if Player.GetAVModF mwAVRepTG base == 0\n				SetQuestItem 0 mwAVRepTG\n			else\n				SetQuestItem 1 mwAVRepTG\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepIL base ) != fbmwILAdvancement.LegionReputation )\n			Player.SetAVModF mwAVRepIL base fbmwILAdvancement.LegionReputation\n			if Player.GetAVModF mwAVRepIL base == 0\n				SetQuestItem 0 mwAVRepIL\n			else\n				SetQuestItem 1 mwAVRepIL\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepIC base ) != fbmwICAdvancement.CultReputation )\n			Player.SetAVModF mwAVRepIC base fbmwICAdvancement.CultReputation\n			if Player.GetAVModF mwAVRepIC base == 0\n				SetQuestItem 0 mwAVRepIC\n			else\n				SetQuestItem 1 mwAVRepIC\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepTT base ) != fbmwTTAdvancement.TempleReputation )\n			Player.SetAVModF mwAVRepTT base fbmwTTAdvancement.TempleReputation\n			if Player.GetAVModF mwAVRepTT base == 0\n				SetQuestItem 0 mwAVRepTT\n			else\n				SetQuestItem 1 mwAVRepTT\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepMT base ) != fbmwMTAdvancement.MTReputation )\n			Player.SetAVModF mwAVRepMT base fbmwMTAdvancement.MTReputation\n			if Player.GetAVModF mwAVRepMT base == 0\n				SetQuestItem 0 mwAVRepMT\n			else\n				SetQuestItem 1 mwAVRepMT\n			endif\n		endif\n	endif\n\nend\n;<CSEBlock>\n;<CSECaretPos> 727 </CSECaretPos>\n;</CSEBlock>", 
