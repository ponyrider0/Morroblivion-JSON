C 00 01 00 07 40 11 00 03 00 72 1B 00 04 00 62 61 73 65 72 1C 00 73 02 00 16 00 1D 00 01 00 19 00 20 72 01 00 58 05 40 0B 00 02 00 72 1B 00 04 00 62 61 73 65 20 30 20 3D 3D 88 14 0A 00 02 00 6E 00 00 00 00 72 1B 00 17 00 02 00 01 00 88 14 0A 00 02 00 6E 01 00 00 00 72 1B 00 19 00 00 00 19 00 00 00 16 00 26 00 06 00 22 00 20 58 69 17 1C 00 01 1B 00 58 05 00 05 40 0D 00 02 00 72 1D 00 04 00 62 61 73 65 56 01 1E 00 02 00 05 1C 00 01 00 07 40 11 00 03 00 72 1D 00 04 00 62 61 73 65 72 1E 00 73 02 00 16 00 1D 00 01 00 19 00 20 72 01 00 58 05 40 0B 00 02 00 72 1D 00 04 00 62 61 73 65 20 30 20 3D 3D 88 14 0A 00 02 00 6E 00 00 00 00 72 1D 00 17 00 02 00 01 00 88 14 0A 00 02 00 6E 01 00 00 00 72 1D 00 19 00 00 00 19 00 00 00 16 00 26 00 06 00 22 00 20 58 69 17 1C 00 01 1B 00 58 05 00 05 40 0D 00 02 00 72 1F 00 04 00 62 61 73 65 56 01 20 00 02 00 05 1C 00 01 00 07 40 11 00 03 00 72 1F 00 04 00 62 61 73 65 72 20 00 73 02 00 16 00 1D 00 01 00 19 00 20 72 01 00 58 05 40 0B 00 02 00 72 1F 00 04 00 62 61 73 65 20 30 20 3D 3D 88 14 0A 00 02 00 6E 00 00 00 00 72 1F 00 17 00 02 00 01 00 88 14 0A 00 02 00 6E 01 00 00 00 72 1F 00 19 00 00 00 19 00 00 00 19 00 00 00 11 00 00 00", 
    "SCTX - Script Source": "scn mwMorroDefaultQuestScript\n\n;Various settings for Morroblivion\nshort QuestMarkersEnabled\nshort HardcoreHarvestMuck\nshort HardcoreHarvestOre\nshort TauntEnabled\nshort FastTravelEnabled\nshort ErrorDetection\n\nshort RestoreGMST\nlong OldControl\nlong OldControl2\n\nshort bIndex\nshort bESP\nshort bBSA\nshort bINI\n\nbegin GameMode\n\n;====================================\n;Morroblivion.ini configuration block\n;====================================\n\n	if GetGameLoaded == 1 || GetGameRestarted == 1\n		if FileExists \"Data\\ini\\Morroblivion.ini\"\n			RunBatchScript \"Data\\ini\\Morroblivion.ini\"\n			set bINI to 0\n		else\n			set bINI to 1\n			set QuestMarkersEnabled to 1\n			set HardcoreHarvestMuck to 0\n			set HardcoreHarvestOre to 0\n			set TauntEnabled to 1\n			set FastTravelEnabled to 1\n			set ErrorDetection to 1\n		endif\n\n		if FastTravelEnabled == 0\n			SetCanFastTravelFromWorld WrldMorrowind 0\n		else\n			SetCanFastTravelFromWorld WrldMorrowind 1\n		endif\n\n		set aaaBoatTransport.fQuestDelayTime to 0.5\n\n		SetEventHandler \"OnActivate\" mwNPCActivateHandler \"activatingRef\"::PlayerRef\n\n;====Error detection part start\n\n		if ErrorDetection == 1\n			set bIndex to 0\n			set bESP to 0\n			set bBSA to 0\n\n			if GetModIndex \"Morrowind_ob.esm\" > 1\n				set bIndex to 1\n			endif\n\n			if IsModLoaded \"Morrowind_ob.esp\" == 0\n				set bESP to 1\n			endif\n\n			if FileExists \"Data\\Morrowind_ob - Meshes.bsa\" == 0 || FileExists \"Data\\Morrowind_ob - Textures.bsa\" == 0 || FileExists \"Data\\Morrowind_ob - Sounds.bsa\" == 0\n				set bBSA to 1\n			endif\n\n			if bIndex > 0 || bESP > 0 || bBSA > 0 || bINI > 0\n				MessageBoxEX \"Morroblivion Errors:%r %{%r- Morrowind_ob.esm is not second in load order.%} %{%r- Morrowind_ob.esp is not loaded.%} %{%r- You are missing at least one of the core Morroblivion BSA archives.%} %{%r- Morroblivion INI file is missing. Using default settings.%}\" bIndex bESP bBSA bINI\n			endif\n		endif\n\n;====Error detection part end\n\n		SetStringGameSettingEX \"sLoadingArea|%e\" ;This has nothing to do with Morroblivion, but bloody hell, this \"Loading area...\" is annoying\n\n	endif\n\n	if RestoreGMST == 1\n\n		SetStringGameSettingEX \"sPCControlsTextPrefix|push\"\n		SetAltControl 18 OldControl\n		SetAltControl 19 OldControl2\n		SetStringGameSettingEX \"sMouseButton7|Button 7\"\n\n		set RestoreGMST to 0\n\n	endif\n\n\n;===================================================================\n;The following block automatically corrects PlayerInMorrowind global\n;===================================================================\n\n\n	if Player.GetInWorldspace WrldMorrowind == 1 || Player.GetInCell Mournhold == 1\n		if PlayerInMorrowind == 0\n			set PlayerInMorrowind to 1\n		endif\n	elseif Player.GetInWorldspace Tamriel == 1 || GetPlayerInSEWorld == 1\n		if PlayerInMorrowind == 1\n			set PlayerInMorrowind to 0\n		endif\n	endif\n\n;===============================================================================\n;The block below handles display of Morroblivion stats in player Misc Stat menu.\n;Will do nothing if AddActorValues OBSE plugin is not installed.\n;===============================================================\n\n	if IsPluginInstalled \"AddActorValues\" == 0\n		return\n	else\n		if eval (( Player.GetAVModF mwAVSlavesFreed base ) != FreedSlavesCounter )\n			Player.SetAVModF mwAVSlavesFreed base FreedSlavesCounter\n			if Player.GetAVModF mwAVSlavesFreed base == 0\n				SetQuestItem 0 mwAVSlavesFreed\n			else\n				SetQuestItem 1 mwAVSlavesFreed\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVThreadsWebspinner base ) != ThreadsWebspinner )\n			Player.SetAVModF mwAVThreadsWebspinner base ThreadsWebspinner\n			if Player.GetAVModF mwAVThreadsWebspinner base == 0\n				SetQuestItem 0 mwAVThreadsWebspinner\n			else\n				SetQuestItem 1 mwAVThreadsWebspinner\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepHT base ) != fbmwHTAdvancement.TelvanniReputation )\n			Player.SetAVModF mwAVRepHT base fbmwHTAdvancement.TelvanniReputation\n			if Player.GetAVModF mwAVRepHT base == 0\n				SetQuestItem 0 mwAVRepHT\n			else\n				SetQuestItem 1 mwAVRepHT\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepHH base ) != fbmwHHAdvancement.HlaaluReputation )\n			Player.SetAVModF mwAVRepHH base fbmwHHAdvancement.HlaaluReputation\n			if Player.GetAVModF mwAVRepHH base == 0\n				SetQuestItem 0 mwAVRepHH\n			else\n				SetQuestItem 1 mwAVRepHH\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepHR base ) != fbmwHRAdvancement.RedoranReputation )\n			Player.SetAVModF mwAVRepHR base fbmwHRAdvancement.RedoranReputation\n			if Player.GetAVModF mwAVRepHR base == 0\n				SetQuestItem 0 mwAVRepHR\n			else\n				SetQuestItem 1 mwAVRepHR\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepMG base ) != fbmwMGAdvancement.MageReputation )\n			Player.SetAVModF mwAVRepMG base fbmwMGAdvancement.MageReputation\n			if Player.GetAVModF mwAVRepMG base == 0\n				SetQuestItem 0 mwAVRepMG\n			else\n				SetQuestItem 1 mwAVRepMG\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepFG base ) != fbmwFGAdvancement.FighterReputation )\n			Player.SetAVModF mwAVRepFG base fbmwFGAdvancement.FighterReputation\n			if Player.GetAVModF mwAVRepFG base == 0\n				SetQuestItem 0 mwAVRepFG\n			else\n				SetQuestItem 1 mwAVRepFG\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepTG base ) != fbmwTGAdvancement.ThiefReputation )\n			Player.SetAVModF mwAVRepTG base fbmwTGAdvancement.ThiefReputation\n			if Player.GetAVModF mwAVRepTG base == 0\n				SetQuestItem 0 mwAVRepTG\n			else\n				SetQuestItem 1 mwAVRepTG\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepIL base ) != fbmwILAdvancement.LegionReputation )\n			Player.SetAVModF mwAVRepIL base fbmwILAdvancement.LegionReputation\n			if Player.GetAVModF mwAVRepIL base == 0\n				SetQuestItem 0 mwAVRepIL\n			else\n				SetQuestItem 1 mwAVRepIL\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepIC base ) != fbmwICAdvancement.CultReputation )\n			Player.SetAVModF mwAVRepIC base fbmwICAdvancement.CultReputation\n			if Player.GetAVModF mwAVRepIC base == 0\n				SetQuestItem 0 mwAVRepIC\n			else\n				SetQuestItem 1 mwAVRepIC\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepTT base ) != fbmwTTAdvancement.TempleReputation )\n			Player.SetAVModF mwAVRepTT base fbmwTTAdvancement.TempleReputation\n			if Player.GetAVModF mwAVRepTT base == 0\n				SetQuestItem 0 mwAVRepTT\n			else\n				SetQuestItem 1 mwAVRepTT\n			endif\n		endif\n\n		if eval (( Player.GetAVModF mwAVRepMT base ) != fbmwMTAdvancement.MTReputation )\n			Player.SetAVModF mwAVRepMT base fbmwMTAdvancement.MTReputation\n			if Player.GetAVModF mwAVRepMT base == 0\n				SetQuestItem 0 mwAVRepMT\n			else\n				SetQuestItem 1 mwAVRepMT\n			endif\n		endif\n	endif\n\nend\n;<CSEBlock>\n;<CSECaretPos> 2045 </CSECaretPos>\n;</CSEBlock>", 
