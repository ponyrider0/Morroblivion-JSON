    "SCTX - Script Source": "scn mwFnCheckInstallation\n\nstring_var msg\nshort show\n\nbegin Function { }\n\n	Let msg := \"Morroblivion Installation check results:%r\"\n\n	if GetModIndex \"Morrowind_ob.esm\" > 1\n		Let msg += \"%r- ERROR: Morrowind_ob.esm is not second in load order. There will be missing landscape in Morroblivion if you don't correct this.\"\n		set show to 1\n	endif\n\n	if IsModLoaded \"Morrowind_ob.esp\" == 0\n		Let msg += \"%r- ERROR: Morrowind_ob.esp is not loaded.\"\n		set show to 1\n	endif\n\n	if FileExists \"Data\\Morrowind_ob - Meshes.bsa\" == 0\n		Let msg += \"%r- ERROR: Morrowind_ob - Meshes.bsa is missing.\"\n		set show to 1\n	endif\n\n	if FileExists \"Data\\Morrowind_ob - Textures.bsa\" == 0\n		Let msg += \"%r- ERROR: Morrowind_ob - Textures.bsa is missing.\"\n		set show to 1\n	endif\n\n	if FileExists \"Data\\Morrowind_ob - Sounds.bsa\" == 0\n		Let msg += \"%r- ERROR: Morrowind_ob - Sounds.bsa is missing.\"\n		set show to 1\n	endif\n\n	if FileExists \"Data\\ini\\Morroblivion.ini\" == 0\n		Let msg += \"%r- ERROR: Morroblivion INI file is missing. Default settings will be used.\"\n		set show to 1\n	endif\n\n	if IsModLoaded \"Morrowind_ob - UCWUS.esp\"\n		if IsPluginInstalled \"OBSE_Kyoma_MenuQue\" == 0\n			Let msg += \"%r- ERROR: MenuQue OBSE plugin is not installed.\"\n			set show to 1\n		endif\n	endif\n\n	if IsModLoaded \"Voice Overs V002.esp\"\n		Let msg += \"%r- WARNING: Voice Overs V002.esp detected. This file is deprecated and should be deactivated.\"\n		set show to 1\n	endif\n\n	if IsModLoaded \"Morrowind_ob - Unique Grass.esp\"\n		Let msg += \"%r- WARNING: Morrowind_ob - Unique Grass.esp detected. This file is deprecated and should be deactivated.\"\n		set show to 1\n	endif\n\n	if IsModLoaded \"Morrowind_ob - Almalexia.esp\"\n		Let msg += \"%r- WARNING: Morrowind_ob - Almalexia.esp detected. This file is deprecated and should be deactivated.\"\n		set show to 1\n	endif\n\n	if IsModLoaded \"Morrowind_ob - MorrowOblivion Eloth Landscape & Grass for v033.esp\"\n		Let msg += \"%r- WARNING: Morrowind_ob - MorrowOblivion Eloth Landscape & Grass for v033.esp detected. This file is deprecated and should be deactivated.\"\n		set show to 1\n	endif\n\n	if show\n		MessageBoxEX $msg\n	endif\n	sv_Destruct msg\n\nend\n;<CSEBlock>\n;<CSECaretPos> 1208 </CSECaretPos>\n;</CSEBlock>", 
