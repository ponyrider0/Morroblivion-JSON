    "SCTX - Script Source": "scriptName mwTRPlagueRock02Script\n\nshort initialized\nshort doonce\nfloat XPos\nfloat CurrentPos\nshort open\n\nbegin gamemode\n\n	if open == 0\n		if ( initialized == 0 )\n			set CurrentPos to GetPos X\n			set initialized to 1\n		endif\n		if (fbmwMSCrimsonPlague.plaguerock == 1)\n			if ( doonce == 0 )\n				PlaySound 0DoorSStoneSOpen\n				set doonce to 1\n			endif\n			if ( GetPos X > -2400 )\n				set XPos to 50 * GetSecondsPassed\n				set CurrentPos to CurrentPos - XPos\n				SetPos X, CurrentPos\n				return\n			else\n				SetPos X, -2400\n				set open to 1\n				Update3D\n			endif\n		endif\n	endif\nend \n;<CSEBlock>\n;<CSECaretPos> 33 </CSECaretPos>\n;</CSEBlock>", 
