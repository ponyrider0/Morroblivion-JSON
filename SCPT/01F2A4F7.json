    "SCTX - Script Source": "ScriptName fbmwttriddlefrost\r\n\r\nshort button\r\nshort QuestionState\r\n\r\n\r\nBegin OnActivate\r\n\r\nif ( QuestionState == 0 )&&getstage fbmwttmountkand<50&&getstage fbmwttmountkand>=25\r\n	MessageBox \"Answer this riddle for me, mortal. Or does thy blood run cold?\" \"Riddle\"\r\n	set QuestionState to 10\r\nelseif getstage fbmwttmountkand<25\r\nmessage \"Ah! First you must pass the Fire Daedra's riddle. Leave this place, unworthy one.\"\r\nelse\r\nmessage \"You have answered my riddle, mortal. Go on to the next challenge before your warm flesh angers me further.\"\r\nendif\r\nEnd\r\n\r\n\r\nBegin GameMode\r\n\r\nif ( QuestionState == 10 )\r\n\r\n	Set button to GetButtonPressed\r\n\r\n	if ( button == 0 )\r\n		MessageBox \"If you lie to me I will slay you with my sword. If you tell me the truth, I will slay you with a spell.\" \"You will slay me with your sword.\" \"You will slay me with a spell.\" \"Don't kill me\"\r\n		set QuestionState to 20\r\n	elseif ( button == 1 )\r\n		set QuestionState to 0\r\n	endif\r\n\r\n	Return\r\n\r\nendif\r\n\r\n;perform cure or give message\r\nif ( QuestionState == 20 )\r\n	\r\n	Set button to GetButtonPressed\r\n\r\n	if ( button == 0 )\r\n		message \"Excellent. You are a worthy opponent. If you do not wish to feel the chills of Oblivion, you may pass.\"\r\n		setstage fbmwttmountkand 50\r\n	endif\r\n	\r\n	if ( button == 1 )\r\n		message \"You have failed the second riddle. You are unworthy.\"\r\n		setav aggression 100\r\n	endif\r\n\r\n	if ( button == 2 )		\r\n		message \"You have failed the second riddle. You are unworthy.\"\r\n		setav aggression 100\r\n	endif\r\n	\r\nendif\r\n\r\nEnd\r\n", 
