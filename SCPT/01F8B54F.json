    "SCTX - Script Source": "scn mwMQAzuraScript\n\nshort state\nfloat timer\nfloat timer2\nfloat timer10\n\nBegin GameMode\n\n	if ( MenuMode == 1 )\n		return\n	endif\n\n	if ( state == -1 )\n		set timer to ( timer + GetSecondsPassed )\n		if ( timer > 9 )\n			if ( GetDisabled == 0 )\n				Disable\n			endif\n		endif\n		return\n	endif\n\n	if ( GetDisabled == 0 )\n		if (GetStage fbmwMQ14 < 20)	;gets set when heart is wacked\n			Disable\n		endif\n	endif\n\n	if ( GetDisabled == 1 )\n		if (	GetStage fbmwMQ14 >= 20 )		;gets set when heart is wacked\n			Enable\n			set state to 2\n		endif\n	endif\n\n;show up\n	if ( state == 2 )\n		if ( GetDistance, Player < 450 )\n			PlayGroup FastForward 1	;fading in/appearing\n;MessageBox \"TEST: WITHIN DISTANCE TO APPEAR\"\n			set state to 5\n			Return\n		endif\n	endif\n\n	if state==5\n		set timer10 to (timer10+GetSecondsPassed)\n		If Timer10>=2.5\n			PlayGroup Forward 1\n			Set state to 6\n		endif\n	endIF\n\n;wait till pc gets closer\n	if ( state == 6 )\n;wait till player gets close\n		if ( GetDistance, Player < 250 )\n			DisablePlayerControls\n;MessageBox \"TEST: WITHIN DISTANCE FOR MOVIE\"\n			set state to 10\n		endif\n	endif\n\n;has appeared, start talking\n	if ( state == 10 )\n		if ( timer < 3 )\n			set timer to ( timer + GetSecondsPassed )\n			return\n		endif\n		PlayBink \"mwend.bik\" 1\n		set state to 20\n\n	endif\n\n\n	if ( state == 20 )\n\n		PlayGroup Backward 1	\n		set state to 30\n	endif\n\n	if ( state == 30 ) && Timer2<3\n		Set timer2 to Timer2+GetSecondsPassed\n\n	Elseif ( state == 30 ) && Timer2>=3\n		Set state to 40\n	EndIF\n\n	if ( state == 40 )\n\n		EnablePlayerControls\n		mwAzuraREF.MoveTo mwHoldingCellMarker\n		mwAzurasRing.Enable\n		fxAzuraBeam1.Enable\n		fxAzuraBeam2.Enable\n		fxAzuraCollisionBox.Enable\n		SetStage fbmwMQ14 50\n		set state to -1\n		set timer to 0\n		\"2akulaSdoorSb\".Rotate z 0\n		\"2akulaSdoorSa\".Rotate z 0\n	endif\n\n\nend \n\n;<CSEBlock>\n;<CSECaretPos> 8 </CSECaretPos>\n;</CSEBlock>", 
