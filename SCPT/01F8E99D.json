    "SCTX - Script Source": "Scn fbmwMSAdultererTarenScript\n\nshort Adulterenabled\n\nbegin OnDeath\n	SetStage fbmwMSAdulterer 120\nend\n\nbegin gamemode\n\n	if ( GetDead == 1 )\n		return\n	elseif GetStage fbmwMSAdulterer == 0\n		return\n	elseif GetStage fbmwMSAdulterer >= 150\n		return\n	endif\n\n	if GetDisabled == 0\n		if ( GameHour < 22 )\n			if ( GameHour > 4 )\n				Disable\n				Return\n			endif\n		endif\n	endif\n\n	if ( GetDistance Player > 1000 )\n		if GetDisabled == 1\n			if ( GameHour > 22 ) || ( GameHour < 4 )\n				Enable\n				set Adulterenabled to 1\n			else\n				Return\n			endif\n		endif\n	endif\n\n	if ( Adulterenabled == 1 )\n		if ( GetDistance Player > 500 )\n			if ( GetDistance Player < 1024 )\n				set fbmwMSAdulterer.walkstate to 1\n				set Adulterenabled to 2\n				SetStage fbmwMSAdulterer 20\n				evp\n				SetForceSneak 1\n			endif\n		endif\n	endif \n\n	if ( fbmwMSAdulterer.tooclose == 0 )\n		if ( fbmwMSAdulterer.walkstate > 0 )\n			if ( GetDistance Player <= 500 )\n				set fbmwMSAdulterer.tooclose to 1\n				evp\n				MessageBox, \"You're too close!  Get farther away before he realizes you're following him!\"\n			endif\n		endif\n	endif\n\n	if ( fbmwMSAdulterer.tooclose == 1 )\n		if (  GetDetectionLevel Player >= 2 )\n			SetForceSneak 0\n			set fbmwMSAdulterer.tooclose to 2\n			StartConversation Player\n		else \n			if ( fbmwMSAdulterer.tooclose < 2 )\n				if GetDetectionLevel Player < 2\n					if GetDistance Player > 500\n						set fbmwMSAdulterer.tooclose to 0\n						MessageBox, \"Good, he didn't see you.\"\n					endif\n				endif\n			endif\n		endif\n	endif\n\n	if fbmwMSAdultererDateMarker.GetDistance fbmwtarenSandoren  <= 71\n		If ( GetDistance Player <= 2000 )\n			set fbmwMSAdulterer.tooclose to 2\n			SetForceSneak 0\n			SetStage fbmwMSAdulterer 30\n		endif\n	endif\nend\n;<CSEBlock>\n;<CSECaretPos> 331 </CSECaretPos>\n;</CSEBlock>", 
