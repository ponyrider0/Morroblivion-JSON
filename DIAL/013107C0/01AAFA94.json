            "TRDT - Response Data": {
                "Emotion Type": "00000000", 
                "Emotion Value": 50, 
                "Unused": "00 00 00 00", 
                "Response number": "06", 
                "Unused": "20 66 61"
            }, 
            "NAM1 - Response Text": "I am not your Lord Dagoth, yet I, too, would say to you... Do you come with weapons to strike me down?", 
            "NAM2 - Actor notes": ""
        }, 
        {
            "TRDT - Response Data": {
                "Emotion Type": "00000000", 
                "Emotion Value": 50, 
                "Unused": "00 00 00 00", 
                "Response number": "07", 
                "Unused": "20 66 61"
