    "Responses": [
        {
            "TRDT - Response Data": {
                "Emotion Type": "00000000", 
                "Emotion Value": 50, 
                "Unused": "00 00 00 00", 
                "Response number": "01", 
                "Unused": "76 69 73"
            }, 
            "NAM1 - Response Text": "I love the people of Morrowind.", 
            "NAM2 - Actor notes": ""
        }, 
        {
            "TRDT - Response Data": {
                "Emotion Type": "00000000", 
                "Emotion Value": 50, 
                "Unused": "00 00 00 00", 
                "Response number": "02", 
                "Unused": "76 69 73"
            }, 
            "NAM1 - Response Text": "I became a god to make their lives more comfortable and secure. I am most close to my faithful followers; I am literally in their hearts and minds.", 
            "NAM2 - Actor notes": ""
        }, 
        {
            "TRDT - Response Data": {
                "Emotion Type": "00000000", 
                "Emotion Value": 50, 
                "Unused": "00 00 00 00", 
                "Response number": "03", 
                "Unused": "76 69 73"
            }, 
            "NAM1 - Response Text": "I feel the most sympathy with House Redoran; they are Dunmer driven by creeds and deeds, like I am.", 
            "NAM2 - Actor notes": ""
        }, 
        {
            "TRDT - Response Data": {
                "Emotion Type": "00000000", 
                "Emotion Value": 50, 
                "Unused": "00 00 00 00", 
                "Response number": "04", 
                "Unused": "76 69 73"
            }, 
            "NAM1 - Response Text": "House Indoril is closer to the compassion and sympathy of Almalexia, a comfortable and secure serenity.", 
            "NAM2 - Actor notes": ""
        }, 
        {
            "TRDT - Response Data": {
                "Emotion Type": "00000000", 
                "Emotion Value": 50, 
                "Unused": "00 00 00 00", 
                "Response number": "05", 
                "Unused": "76 69 73"
            }, 
            "NAM1 - Response Text": "House Telvanni matches the disposition of my brother Sotha Sil -- iconoclastic, profane, unconventional.", 
            "NAM2 - Actor notes": ""
        }, 
        {
            "TRDT - Response Data": {
                "Emotion Type": "00000000", 
                "Emotion Value": 50, 
                "Unused": "00 00 00 00", 
                "Response number": "06", 
                "Unused": "76 69 73"
            }, 
            "NAM1 - Response Text": "House Hlaalu represents the future of the Dunmer, integrated into the sophisticated mainstream of the traditionless, raceless, godless culture of the Empire.", 
            "NAM2 - Actor notes": ""
        }, 
        {
            "TRDT - Response Data": {
                "Emotion Type": "00000000", 
                "Emotion Value": 50, 
                "Unused": "00 00 00 00", 
                "Response number": "07", 
                "Unused": "20 66 61"
            }, 
            "NAM1 - Response Text": "House Dres represents the past of pre-Tribunal Great House culture, a persistent tradition of Daedra- and ancestor-worshipping civilized Dunmer clans", 
            "NAM2 - Actor notes": ""
        }, 
        {
            "TRDT - Response Data": {
                "Emotion Type": "00000000", 
                "Emotion Value": 50, 
                "Unused": "00 00 00 00", 
                "Response number": "08", 
                "Unused": "20 66 61"
            }, 
            "NAM1 - Response Text": "And I even love the Ashlanders for their preservation of the most ancient barbarian tribal traditions of the Dunmer who first settled Morrowind.", 
            "NAM2 - Actor notes": ""
        }
    ], 
